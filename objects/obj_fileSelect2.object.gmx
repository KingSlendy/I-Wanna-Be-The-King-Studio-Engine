            <string>var length = array_length_1d(global.difficultyNames);

if (menu == 0) {
    if (scr_buttonPressed(global.menuUpButton)) {
        select[menu]--;
        audio_play_sound(snd_doubleJump, 0, false);
    }
        
    if (scr_buttonPressed(global.menuDownButton)) {
        select[menu]++;
        audio_play_sound(snd_doubleJump, 0, false);
    }

    select[menu] += global.totalSaves;
    select[menu] %= global.totalSaves;
    
    if (scr_buttonPressed(global.menuAcceptButton)) {
        select[1] = cond_exp(file_exists(scr_saveName(select[menu])), length - 1, 0);
        menu = 1;
        audio_play_sound(snd_jump, 0, false);
    }
    
    if (scr_buttonPressed(global.menuBackButton))
        room_goto(rm_title);
} else if (menu == 1) {
    if (scr_buttonPressed(global.menuLeftButton)) {
        select[menu]--;
        audio_play_sound(snd_doubleJump, 0, false);
    }
    
    if (scr_buttonPressed(global.menuRightButton)) {
        select[menu]++;
        audio_play_sound(snd_doubleJump, 0, false);
    }
        
    if (!file_exists(scr_saveName(select[0])))
        length--;
        
    select[menu] += length;
    select[menu] %= length;
    
    if (scr_buttonPressed(global.menuAcceptButton)) {
        global.saveNum = select[0];
        scr_startGame(select[menu]);
    }
    
    if (scr_buttonPressed(global.menuBackButton))
        menu = 0;
}

if (scr_buttonPressed(global.menuOptionsButton))
    room_goto(rm_options);
